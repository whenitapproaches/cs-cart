msgid "Notify vendor"
msgstr "Powiadom sprzedawcę"

msgctxt "SettingsOptions::vendor_communication::notify_customer"
msgid "Notify customer"
msgstr "Powiadom klienta"

msgctxt "SettingsOptions::vendor_communication::show_on_product"
msgid "Contact button on product pages"
msgstr "Przycisk kontaktu na stronach produktów"

msgctxt "SettingsOptions::vendor_communication::show_on_vendor"
msgid "Contact button in vendor microstore"
msgstr "Przycisk kontaktu w minisklepie sprzedawcy"

msgctxt "SettingsTooltips::vendor_communication::notify_admin"
msgid "Send an email to root administrator about every newly-posted message."
msgstr "Wyślij e-mail do głównego administratora o każdej nowo opublikowanej wiadomości."

msgctxt "SettingsTooltips::vendor_communication::notify_vendor"
msgid "Send emails to vendor's contact email address about newly-posted messages."
msgstr "Wysyłaj wiadomości e-mail na kontaktowy adres e-mail sprzedawcy o nowo przesłanych wiadomościach."

msgctxt "SettingsTooltips::vendor_communication::notify_customer"
msgid "Send emails to customers about newly-posted messages in their threads."
msgstr "Wysyłaj wiadomości e-mail do klientów o nowo przesłanych wiadomościach w wątkach."

msgctxt "SettingsTooltips::vendor_communication::show_on_product"
msgid "Show the \"Contact vendor\" button on product pages."
msgstr "Pokaż przycisk \"Kontakt ze sprzedawcą\" na kartach produktów."

msgctxt "SettingsTooltips::vendor_communication::show_on_vendor"
msgid "Show the \"Contact vendor\" button in vendors' microstores."
msgstr "Pokaż przycisk \"Kontakt ze sprzedawcą\" w mikrosklepach sprzedawców."

