msgctxt "Addons::name::vendor_communication"
msgid "Customer-to-Vendor Communication"
msgstr "Komunikacja klienta ze sprzedawcą"

msgctxt "Addons::description::vendor_communication"
msgid "Allows customers to communicate with vendors directly via the store's interface."
msgstr "Umożliwia komunikację ze sprzedawcami bezpośrednio za pomocą interfejsu sklepu."

msgctxt "SettingsSections::vendor_communication::general"
msgid "General"
msgstr "Ogólne"

msgctxt "SettingsSections::vendor_communication::notifications"
msgid "Notifications"
msgstr "Powiadomienia"

